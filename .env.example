# VocalIQ Environment Variables
# Copy this file to .env and fill in your actual values

# Environment
ENVIRONMENT=development
DEBUG=true

# Security Keys (Generate new ones for production\!)
SECRET_KEY=your-secret-key-here-change-this-in-production
JWT_SECRET_KEY=your-jwt-secret-here-change-this-in-production
ENCRYPTION_KEY=your-encryption-key-here-change-this
ADMIN_PASSWORD=change-this-admin-password

# Database
DB_PASSWORD=secure-database-password-here
DATABASE_URL=postgresql://vocaliq:${DB_PASSWORD}@localhost:5432/vocaliq

# Redis
REDIS_URL=redis://localhost:6379/0

# OpenAI (Required for GPT-4 and Whisper)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key-here

# ElevenLabs (Required for Text-to-Speech)
# Get from: https://elevenlabs.io/api
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here
ELEVENLABS_VOICE_ID=ROYWaqtov1Lot9vQBM3y

# Twilio (Required for Voice Calls)
# Get from: https://console.twilio.com
TWILIO_ACCOUNT_SID=ACyour-twilio-account-sid-here
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here
TWILIO_PHONE_NUMBER=+1234567890

# ngrok URL (for local development)
# Update this when you start ngrok
NGROK_URL=https://your-ngrok-url.ngrok-free.app

# Monitoring (Optional)
GRAFANA_PASSWORD=admin

# API Settings
API_BASE_URL=http://localhost:8001
FRONTEND_URL=http://localhost:5173

# Feature Flags
FEATURE_OUTBOUND_CALLS=true
FEATURE_INBOUND_CALLS=true
FEATURE_CALENDAR_INTEGRATION=true
FEATURE_ANALYTICS=true
EOF < /dev/null